package shelfStorageFrame;

public class PutGet {

	private static PutGetDoc doc2;

	public static void PG(PutGetDoc doc1)
	{
		// реализовать функцию сохранения документа в склад
		doc2 = doc1;
		
		String sql = "EXEC	[dbo].[moveGoods]"
		+ "				@input_id_shelf = "+","
		+ "				@input_id_emp = "+","
		+ "				@input_id_art = "+","
		+ "				@input_count = "+","
		+ "				@input_move = "

			// убираем товар с полки
			doc2.getIdEmp();
			doc2.getIdArt();
			doc2.getCount();
			doc2.getAdd(); 

	}
}
