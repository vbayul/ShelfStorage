package shelfStorageFrame;

import db.ExecProc;

public class PutGet {

	private static PutGetDoc doc2;

	public static void PG(PutGetDoc doc1)
	{
		// реализовать функцию сохранения документа в склад
		doc2 = doc1;
		
		String sql = "EXEC	[dbo].[moveGoods]"
		+ "				@input_id_shelf = "+PutGetDoc.getShelfId(doc2.getGrow(),doc2.getRow(),doc2.getColumns())+","
		+ "				@input_id_emp = "+doc2.getIdEmp()+","
		+ "				@input_id_art = "+doc2.getIdArt()+","
		+ "				@input_count = "+doc2.getCount()+","
		+ "				@input_move = "+doc2.getAdd();
		ExecProc.exec(sql);

			/* убираем товар с полки
			doc2.getIdEmp();
			doc2.getIdArt();
			doc2.getCount();
			doc2.getAdd(); 
			 */
	}
}
