package shelfStorage;

public class PutGetDocument {
	private String barcode;
	private String articul;
	private String count;
	private String grow;
	private String row;
	private String columns;
	private int idEmp;
	private int idArt;
	
	public PutGetDocument(String barcode, String articul, String count, String grow, String row, String columns) {
		this.barcode = barcode;
		this.articul = articul;
		this.count = count;
		this.grow = grow;
		this.row = row;
		this.columns = columns;
	}
	
	public String getCount() {
		return count;
	}

	public int getIdEmp() {
		// через доступ к базе, написать вывод id
		
		return idEmp;
	}

	public int getIdArt() {

		// через доступ к базе, написать вывод id
		return idArt;
	}
}
